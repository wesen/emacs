(with-accessors (@var{slot-entry}@sub1{}, @dots{} @var{slot-entry}@subs1{n})
@var{instance-form} @var{form}@sub1{} @dots{} @var{form}@subs1{k})
@end lisp

expands into the equivalent of

@lisp
(let ((in @var{instance-form}))
  (symbol-macrolet (@var{Q}@sub1{} @dots{} @var{Q}@subs1{n}) @var{form}@sub1{} @dots{} @var{form}@subs1{k}))
@end lisp

where @var{Q}@subs1{i} is

@lisp
(@var{variable-name}@subs1{i} () (@var{accessor-name}@subs1{i} in))
@end lisp

@end macro

@macro withslots {}
